    android:id="@+id/nav_graph_main"
    app:startDestination="@id/fragmentOnboarding">

    <fragment
        android:id="@+id/fragmentOnboarding"
        android:name="com.zhdanon.skillcinema.presentation.onboarding.FragmentOnboarding"
